msgid "plugins.generic.googleTagManager.displayName"
msgstr "Google Tag Manager Plugin"

msgid "plugins.generic.googleTagManager.description"
msgstr "Integrate with Google Tag Manager. Google Tag Manager is a tag management system created by Google to manage JavaScript and HTML tags used for tracking and analytics on websites. Requires that you have already setup a Google Tag Manager account. Please see the <a href=\"https://tagmanager.google.com/\" title=\"Google Tag Manager site\">Google Tag Manager site</a> for more information."

msgid "plugins.generic.googleTagManager.manager.settings.description"
msgstr "<p>With this plugin enabled Google Tag Manager may be used to collect and analyze web site usage and traffic. Please note that this plugin requires that you have already setup a Google Tag Manager account. Please see the <a href=\"https://tagmanager.google.com/\" title=\"Google Tag Manager site\">Google Tag Manager site</a> for more information.</p>"

msgid "plugins.generic.googleTagManager.manager.settings.googleTagManagerId"
msgstr "Account number"

msgid "plugins.generic.googleTagManager.manager.settings.googleTagManagerIdRequired"
msgstr "Please enter an account number."
